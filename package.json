{
  "name": "mutent-mongodb",
  "version": "0.12.0",
  "description": "MongoDB adapter for Mutent",
  "type": "module",
  "main": "./mutent-mongodb.cjs",
  "types": "./mutent-mongodb.d.ts",
  "exports": {
    "import": "./mutent-mongodb.mjs",
    "require": "./mutent-mongodb.cjs",
    "types": "./mutent-mongodb.d.ts"
  },
  "files": [
    "mutent-mongodb.cjs",
    "mutent-mongodb.mjs",
    "mutent-mongodb.d.ts"
  ],
  "scripts": {
    "lint": "standard **/*.mjs",
    "build": "rollup -c",
    "test": "c8 ava",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "keywords": [
    "mutent",
    "mongo",
    "mongodb",
    "collection"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "peerDependencies": {
    "mongodb": "^5.0.0",
    "mutent": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^5.3.0",
    "c8": "^8.0.0",
    "mongodb": "^5.6.0",
    "mql-match": "^0.7.1",
    "mutent": "^5.0.1",
    "rollup": "^3.25.1",
    "standard": "^17.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mutent-mongodb.git"
  }
}
