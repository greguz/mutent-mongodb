{
  "name": "mutent-mongodb",
  "version": "0.10.2",
  "description": "MongoDB adapter for Mutent",
  "type": "module",
  "main": "./mutent-mongodb.cjs",
  "exports": {
    "import": "./mutent-mongodb.mjs",
    "require": "./mutent-mongodb.cjs"
  },
  "types": "./mutent-mongodb.d.ts",
  "files": [
    "mutent-mongodb.cjs",
    "mutent-mongodb.mjs",
    "mutent-mongodb.d.ts"
  ],
  "scripts": {
    "lint": "standard **/*.mjs",
    "build": "rollup -c",
    "test": "ava",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "mutent",
    "mongo",
    "mongodb",
    "collection"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "peerDependencies": {
    "mutent": "5.0.0-9"
  },
  "devDependencies": {
    "ava": "4.0.0-alpha.2",
    "mongodb": "^4.1.3",
    "mutent": "5.0.0-9",
    "rollup": "^2.58.0",
    "standard": "^16.0.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mutent-mongodb.git"
  }
}
