{
  "name": "mutent-mongodb",
  "version": "0.11.1",
  "description": "MongoDB adapter for Mutent",
  "type": "module",
  "main": "./mutent-mongodb.cjs",
  "types": "./mutent-mongodb.d.ts",
  "exports": {
    "import": "./mutent-mongodb.mjs",
    "require": "./mutent-mongodb.cjs",
    "types": "./mutent-mongodb.d.ts"
  },
  "files": [
    "mutent-mongodb.cjs",
    "mutent-mongodb.mjs",
    "mutent-mongodb.d.ts"
  ],
  "scripts": {
    "lint": "standard **/*.mjs",
    "build": "rollup -c",
    "test": "ava",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "mutent",
    "mongo",
    "mongodb",
    "collection"
  ],
  "author": "Giacomo Gregoletto",
  "license": "MIT",
  "dependencies": {
    "mongodb": "^4.11.0"
  },
  "peerDependencies": {
    "mutent": "^5.0.0"
  },
  "devDependencies": {
    "ava": "^5.0.1",
    "mutent": "^5.0.0",
    "rollup": "^3.2.5",
    "standard": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/greguz/mutent-mongodb.git"
  }
}
